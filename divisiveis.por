programa {
  funcao inicio() {
    //Declaração de variavel
    inteiro numero1, numero2

    //Coleta
    escreva("Digite 02 numeros respectivamente: \n")
    leia(numero1, numero2)

    //Processamento
    se(numero1 % numero2 == 0){
      //SAIDA
      escreva("O primeiro numero é divisivel pelo segundo!")
    }
    senao{
      //SAIDA
      escreva("O primeiro numero NÃO é divisivel pelo segundo!")
    }




  }
}
